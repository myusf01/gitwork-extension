{
  "name": "git-work-commit",
  "displayName": "Git Work Commit",
  "description": "Custom Git Work Commit button",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "build": "vsce package --no-yarn"
  },
  "activationEvents": [
    "onCommand:extension.gitWorkCommit",
    "onFileSystem:file"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{  
      "command": "extension.gitWorkCommit",
      "title": "Git Work Commit"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myusf01/gitwork-extension.git"
  }
}